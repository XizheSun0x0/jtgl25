.privacy-mask {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,0.5);
  z-index: 9999;
  display: flex;
  justify-content: center;
  align-items: center;
}

.privacy-container {
  width: 80%;
  background: #fff;
  border-radius: 16rpx;
  padding: 40rpx;
}

.privacy-link {
  color: #07C160;
  text-decoration: underline;
}

.button-group {
  display: flex;
  justify-content: space-between;
  margin-top: 40rpx;
}

.disagree-btn {
  background: #f5f5f5;
  color: #666;
  width: 48%;
}

.agree-btn {
  background: #07C160;
  color: white;
  width: 48%;
}